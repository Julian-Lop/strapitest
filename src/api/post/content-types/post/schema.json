      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "Likes": {
      "type": "integer"
    },
    "Dislikes": {
      "type": "integer"
    },
    "Compartido": {
      "type": "integer"
    }
  }
